{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "no-console":"off",
        "quotes": [2, "double", { "avoidEscape": true }],
        "no-underscore-dangle":"off"
    }
}
